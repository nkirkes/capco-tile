@model CAPCO.Infrastructure.Domain.StoreLocation

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>StoreLocation</legend>

    <div class="display-label">Name</div>
    <div class="display-field">@Model.Name</div>

    <div class="display-label">LocationDetails</div>
    <div class="display-field">@Model.LocationDetails</div>

    <div class="display-label">Description</div>
    <div class="display-field">@Model.Description</div>

    <div class="display-label">GoogleMapEmbedCode</div>
    <div class="display-field">@Model.GoogleMapEmbedCode</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>


